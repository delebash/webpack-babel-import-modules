    "node-sass": "^3.4.2",
    "postcss-loader": "^0.8.2",
    "protractor": "^3.2.1",
    "raw-loader": "^0.5.1",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1",
    "babel-eslint": "^5.0.0",
    "exports-loader": "^0.6.3",
    "expose-loader": "^0.7.1"
