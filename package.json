  "bugs": {
    "url": "https://github.com/mrooding/webpack-babel-integration-example/issues"
  },
  "homepage": "https://github.com/mrooding/webpack-babel-integration-example#readme",
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.7.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-es2015-loose-rollup": "^7.0.0",
    "babel-preset-stage-1": "^6.5.0",
    "css-loader": "^0.23.1",
    "eslint": "^2.8.0",
    "extract-text-webpack-plugin": "^1.0.1",
